        <value>RUNNING</value>
    </property>

    <property>
        <name>yarn.scheduler.capacity.root.bonc.acl_submit_applications</name>
        <value> </value>
    </property>

    <property>
        <name>yarn.scheduler.capacity.root.bonc.acl_administer_queue</name>
        <value> </value>
    </property>

    <property>
        <name>yarn.scheduler.capacity.root.bonc.queues</name>
        <value>dev1,dev2,dev3,dev4,dev5</value>
    </property>

    <!--root.bonc.dev1 START-->
    <property>
        <name>yarn.scheduler.capacity.root.bonc.dev1.capacity</name>
        <value>20</value>
    </property>

    <property>
        <name>yarn.scheduler.capacity.root.bonc.dev1.user-limit-factor</name>
        <value>1.0</value>
