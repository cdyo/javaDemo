    </property>

    <property>
        <name>yarn.scheduler.capacity.root.bonc.dev1.state</name>
        <value>RUNNING</value>
    </property>

    <property>
        <name>yarn.scheduler.capacity.root.bonc.dev1.acl_submit_applications</name>
        <value>dev1 dev1</value>
    </property>

    <property>
        <name>yarn.scheduler.capacity.root.bonc.sxbdb.acl_administer_queue</name>
        <value>dev1 dev1</value>
    </property>


    <!--root.bonc.dev2 START-->
    <property>
        <name>yarn.scheduler.capacity.root.bonc.dev2.capacity</name>
        <value>20</value>
        <description>Default queue target capacity.</description>
