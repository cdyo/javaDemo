    </property>

    <property>
        <name>yarn.scheduler.capacity.root.bonc.dev4.acl_submit_applications</name>
        <value>dev4 dev4</value>
    </property>

    <property>
        <name>yarn.scheduler.capacity.root.bonc.dev4.acl_administer_queue</name>
        <value>dev4 dev4</value>
